procedure TForm2.SWhiteTusheMouseDown(Sender: TObject;
  Button: TMouseButton; Shift: TShiftState; X, Y: Integer);
begin
TShape(sender).Brush.Color:=clRed;
Label1.Caption:='Nota - Do (1)';
Memo1.Lines.Add('Последняя нажатая нота - До ( 1 октавы )');
Windows.Beep(261,200);
end;

procedure TForm2.SWhiteTusheMouseUp(Sender: TObject; Button: TMouseButton;
  Shift: TShiftState; X, Y: Integer);
begin
TShape(sender).Brush.Color:=clWhite;
end;

