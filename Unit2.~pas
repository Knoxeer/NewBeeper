procedure TForm2.FormKeyPress(Sender: TObject; var Key: Char);
begin
if key='q' then Windows.Beep(261,200); // Memo1.Lines.Add('Последняя нажатая нота - До ( 1 октавы )');
if key='w' then Windows.Beep(293,200); // Memo1.Lines.Add('Последняя нажатая нота - Ре ( 1 октавы )');
if key='e' then Windows.Beep(329,200); // Memo1.Lines.Add('Последняя нажатая нота - Ми ( 1 октавы )');
if key='r' then Windows.Beep(349,200); // Memo1.Lines.Add('Последняя нажатая нота - Фа ( 1 октавы )');
if key='t' then Windows.Beep(392,200); // Memo1.Lines.Add('Последняя нажатая нота - Соль ( 1 октавы )');
if key='y' then Windows.Beep(440,200); // Memo1.Lines.Add('Последняя нажатая нота - Ля ( 1 октавы )');
if key='u' then Windows.Beep(493,200); // Memo1.Lines.Add('Последняя нажатая нота - Си ( 1 октавы )');
if key='a' then Windows.Beep(523,200); // Memo1.Lines.Add('Последняя нажатая нота - До ( 2 октавы )');
if key='s' then Windows.Beep(587,200); // Memo1.Lines.Add('Последняя нажатая нота - Ре ( 2 октавы )');
if key='d' then Windows.Beep(659,200); // Memo1.Lines.Add('Последняя нажатая нота - Ми ( 2 октавы )');
if key='f' then Windows.Beep(698,200); // Memo1.Lines.Add('Последняя нажатая нота - Фа ( 2 октавы )');
if key='g' then Windows.Beep(783,200); // Memo1.Lines.Add('Последняя нажатая нота - Соль ( 2 октавы )');
if key='h' then Windows.Beep(880,200); // Memo1.Lines.Add('Последняя нажатая нота - Ля ( 2 октавы )');
if key='j' then Windows.Beep(987,200); // Memo1.Lines.Add('Последняя нажатая нота - Си ( 2 октавы )');
if key='z' then Windows.Beep(1046,200); //Memo1.Lines.Add('Последняя нажатая нота - До ( 3 октавы )');
if key='x' then Windows.Beep(1174,200); //Memo1.Lines.Add('Последняя нажатая нота - Ре ( 3 октавы )');
if key='c' then Windows.Beep(1318,200); //Memo1.Lines.Add('Последняя нажатая нота - Ми ( 3 октавы )');
if key='v' then Windows.Beep(1396,200); //Memo1.Lines.Add('Последняя нажатая нота - Фа ( 3 октавы )');
if key='b' then Windows.Beep(1568,200); //Memo1.Lines.Add('Последняя нажатая нота - Соль ( 3 октавы )');
if key='n' then Windows.Beep(1760,200); //Memo1.Lines.Add('Последняя нажатая нота - Ля ( 3 октавы )');
if key='m' then Windows.Beep(1975,200); //Memo1.Lines.Add('Последняя нажатая нота - Си ( 3 октавы )');
if key=',' then Windows.Beep(2093,200); //Memo1.Lines.Add('Последняя нажатая нота - До ( 4 октавы )');
//
end;

